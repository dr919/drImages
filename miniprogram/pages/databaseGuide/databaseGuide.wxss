page {
  width: 100%;
  height: 100%;
}

.main {
  width: 100%;
  height: 100%;
  /* background:url('../../images/bgimg.png') no-repeat; */
  /* background-size:100% 100%; */
  margin: 0 auto;
  overflow: hidden;
  text-align: center;
  position: relative;
}

.bgImg {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
}

.time {
  color: #94949d;
  font-size: 14px;
  margin: 53% auto 0px;
  text-align: center;
  position: relative;
  z-index: 9;
}

.arricon {
  width: 150px;
  height: 150px;
  margin: 0 auto;
  animation: mymove 0.7s infinite alternate ease-in;
  -webkit-animation: mymove 0.7s infinite alternate ease-in;
  margin-top: 40rpx;
  position: relative;
  z-index: 9;
}

@keyframes mymove {
  from {
    transform: scale(1.04);
  }

  to {
    transform: scale(0.9);
  }
}

@-webkit-keyframes mymove {
  from {
    transform: scale(1.04);
  }

  to {
    transform: scale(0.9);
  }
}

